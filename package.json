{
  "name": "stay-ahead",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "lint": "eslint \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json}\" \"tests/**/*.{js,ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,json}\" \"tests/**/*.{js,ts,json}\"",
    "docs": "jsdoc -c jsdoc.json",
    "typecheck": "tsc --noEmit",
    "build": "npm run lint && npm run typecheck && npm run test && npm run e2e",
    "dev": "node --watch src/app.js",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@playwright/test": "^1.43.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "vibe-coding",
    "agent-driven",
    "template",
    "javascript",
    "typescript",
    "testing"
  ]
}
